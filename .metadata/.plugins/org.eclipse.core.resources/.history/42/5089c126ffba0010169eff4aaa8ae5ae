package dto;

public class ScheduleDTO {

    private Integer id;
    private Integer physicianId;
    private Integer patientId;
    private String day;
    private String time;

    public ScheduleDTO() {}

    public ScheduleDTO(Integer id, Integer physicianId, Integer patientId, String day, String time) {
        this.id = id;
        this.physicianId = physicianId;
        this.patientId = patientId;
        this.day = day;
        this.time = time;
    }

    public Integer getId() { return id; }
    public void setId(Integer id) { this.id = id; }

    public Integer getPhysicianId() { return physicianId; }
    public void setPhysicianId(Integer physicianId) { this.physicianId = physicianId; }

    public Integer getPatientId() { return patientId; }
    public void setPatientId(Integer patientId) { this.patientId = patientId; }

    public String getDay() { return day; }
    public void setDay(String day) { this.day = day; }

    public String getTime() { return time; }
    public void setTime(String time) { this.time = time; }
}
